<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apjt.android.helloworld3" android:debuggable="true">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:debuggable="true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
                  android:debuggable="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" android:debuggable="true" />

                <category android:name="android.intent.category.LAUNCHER" android:debuggable="true" />
            </intent-filter>
        </activity>
    </application>

</manifest>
